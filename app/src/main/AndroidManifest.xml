<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.asclepius.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.RegisterAsDoctor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.AddFamilyMember"></activity>
        <activity android:name=".activities.VerifyMobileNumber" />
        <activity android:name=".activities.BookAppointment" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.CreateAccount" />
        <activity android:name=".activities.OnBoardActivity"></activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>